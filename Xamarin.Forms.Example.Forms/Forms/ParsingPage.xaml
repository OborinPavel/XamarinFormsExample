<?xml version="1.0" encoding="UTF-8"?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="Xamarin.Forms.Example.ParsingPage" Title="Parse data object">
	<ContentPage.Content>
		<StackLayout Padding="20" VerticalOptions="FillAndExpand">
			<Label x:Name="xmlLabel" Text="http://maps.googleapis.com/maps/api/geocode/xml?address=250+Swanston+St,+Melbourne+VIC+3000&amp;sensor=false" />
			<Button x:Name="btnParseXml" Text="Parse xml" Clicked="ParseClicked"/>
			<Label x:Name="jsonLabel" Text="http://maps.googleapis.com/maps/api/geocode/json?address=250+Swanston+St,+Melbourne+VIC+3000&amp;sensor=false" />
			<Button x:Name="btnPaseJsonStatic" Text="Parse JSON" Clicked="ParseClicked"/>
		</StackLayout>
	</ContentPage.Content>
</ContentPage>
